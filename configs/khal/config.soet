[calendars]

[[private]]
path = /home/ahacad/.local/share/khal/calendars/private
type = calendar
color = dark green
[[localpersonal]]
path = /home/ahacad/.calendars/Personal/local
type = calendar
color = dark magenta


[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
default_calendar = localpersonal
