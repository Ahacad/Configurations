super + Return
        st

super + c
        rofi -show drun

super + shift + r
        pkill -USR1 -x sxhkd

super + shift + e
        bspc wm -r

Xf86MonBrightnessUp
        xbacklight -inc 10

Xf86MonBrightnessDown
        xbacklight -dec 10

super + alt + {q,r}
        bspc {quit, wm -r}

super + {_,shift + }q
        bspc node -{c, k}

super + {d,shift + d,s,f}
	bspc node -t {tiled,pseudo_tiled,floating,fullscreen}

